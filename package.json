{
  "name": "platav3-finance-app",
  "version": "1.0.0",
  "description": "AI-powered personal finance tracking application",
  "main": "server/src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "cd client && npm run dev",
    "server:dev": "cd server && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "cd client && npm run typecheck && cd ../server && npm run typecheck",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:seed": "cd server && npx prisma db seed",
    "db:studio": "cd server && npx prisma studio"
  },
  "keywords": ["finance", "ai", "personal-finance", "budgeting", "savings"],
  "author": "Diego Bexar",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "jest": "^29.6.2"
  }
}